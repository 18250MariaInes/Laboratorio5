<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".EditarContacto">

    <TextView
            android:text="Editar contacto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="113dp"
            android:layout_marginStart="113dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="112dp" android:layout_marginRight="112dp" android:layout_marginTop="34dp"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="32dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/nombren"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="84dp"
            android:layout_marginRight="84dp" android:layout_marginTop="88dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2" android:hint="Nombre"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="none|textPersonName"
            android:ems="10"
            android:id="@+id/telefonon"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="84dp" android:layout_marginRight="84dp" android:hint="Telefono"
            android:layout_marginTop="29dp" app:layout_constraintTop_toBottomOf="@+id/nombren"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/correon"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="84dp" android:layout_marginRight="84dp" android:layout_marginTop="27dp"
            app:layout_constraintTop_toBottomOf="@+id/telefonon"/>
    <Button
            android:text="Editar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="116dp"
            android:layout_marginStart="116dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="104dp" android:layout_marginRight="104dp" android:layout_marginBottom="29dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.6"
            android:onClick="editarcontactofinal"/>
    <ImageButton
            android:layout_width="90dp"
            android:layout_height="69dp" app:srcCompat="@android:drawable/ic_menu_camera"
            android:id="@+id/imageN" android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="107dp" android:layout_marginRight="107dp"/>
</android.support.constraint.ConstraintLayout>