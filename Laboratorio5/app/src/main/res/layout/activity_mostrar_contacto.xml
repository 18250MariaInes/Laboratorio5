<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MostrarContacto">


    <Button
            android:text="Regresar a contactos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonRegreso" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="109dp" android:layout_marginStart="109dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="110dp"
            android:layout_marginRight="110dp"
            app:layout_constraintBottom_toBottomOf="parent" android:onClick="regresarmenu"
            app:layout_constraintHorizontal_bias="0.464" android:layout_marginBottom="28dp"/>
    <TextView
            android:text="Nombre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/NombreSelected"
            android:textSize="24dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="161dp" android:layout_marginStart="161dp" android:layout_marginBottom="43dp"
            app:layout_constraintBottom_toTopOf="@+id/telefonoSelected"/>/>
    <TextView
            android:text="telefono"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/telefonoSelected"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="176dp" android:layout_marginStart="176dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="183dp"
            android:layout_marginRight="183dp" app:layout_constraintHorizontal_bias="0.454"
            android:textSize="24dp"
            android:textStyle="italic"

            android:textColor="#1C28C1" android:layout_marginBottom="34dp"
            app:layout_constraintBottom_toTopOf="@+id/correoSelected"/>
    />
    <TextView
            android:text="Correo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/correoSelected"
            android:textSize="24dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="164dp"
            android:layout_marginRight="164dp"
            android:textStyle="italic"
            android:textColor="#1C28C1" tools:layout_editor_absoluteY="370dp"
    />/>/>
    <TextView
            android:text="Ver contacto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="164dp"
            android:layout_marginStart="164dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="164dp" android:layout_marginRight="164dp" android:layout_marginTop="48dp"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="32sp"/>
    <Button
            android:text="Editar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editarButton" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="148dp" android:layout_marginStart="148dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="148dp"
            android:layout_marginRight="148dp" android:layout_marginTop="17dp"
            app:layout_constraintTop_toBottomOf="@+id/correoSelected" android:layout_marginBottom="24dp"
            app:layout_constraintBottom_toTopOf="@+id/buttonRegreso" android:onClick="editarContacto"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="1.0"/><![CDATA[
    />
]]>
</android.support.constraint.ConstraintLayout>