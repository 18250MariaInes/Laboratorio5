<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MostrarContacto">


    <Button
            android:text="Regresar a contactos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonRegreso" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="109dp" android:layout_marginStart="109dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="110dp"
            android:layout_marginRight="110dp"
            app:layout_constraintBottom_toBottomOf="parent" android:onClick="regresarmenu"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginBottom="60dp"/>
    <TextView
            android:text="Nombre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/NombreSelected" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="176dp" android:layout_marginStart="176dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="185dp"
            android:layout_marginRight="185dp" app:layout_constraintHorizontal_bias="0.388"
            android:textSize="24dp"
            android:layout_marginBottom="61dp" app:layout_constraintBottom_toTopOf="@+id/telefonoSelected"
            android:layout_marginTop="98dp" app:layout_constraintTop_toBottomOf="@+id/textView6"/>/>
    <TextView
            android:text="telefono"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/telefonoSelected"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="176dp" android:layout_marginStart="176dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="183dp"
            android:layout_marginRight="183dp" app:layout_constraintHorizontal_bias="0.333"
            android:textSize="24dp" android:layout_marginBottom="58dp"
            app:layout_constraintBottom_toTopOf="@+id/correoSelected"
            android:textStyle="italic"

            android:textColor="#1C28C1"/>
    />
    <TextView
            android:text="Correo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/correoSelected"
            android:textSize="24dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="157dp" android:layout_marginStart="157dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="158dp"
            android:layout_marginRight="158dp" android:layout_marginBottom="89dp"
            app:layout_constraintBottom_toTopOf="@+id/buttonRegreso"
            android:textStyle="italic"
            android:textColor="#1C28C1"/>/>/>
    <TextView
            android:text="Ver contacto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="164dp"
            android:layout_marginStart="164dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="164dp" android:layout_marginRight="164dp" android:layout_marginTop="48dp"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="32sp"/>/>
</android.support.constraint.ConstraintLayout>